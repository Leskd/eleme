{
  "name": "eleme",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "shaon",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev-renderer": "node build/dev-server.js",
    "ele-dev": "cross-env NODE_ENV=development electron -r babel-register ./src/main/main",
    "dev": "npm run dev-renderer && npm run ele-dev",
    "build-renderer": "node build/build.js",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --colors",
    "build": "concurrently \"npm run build-renderer\" \"npm run build-main\"",
    "start": "electron .",
    "pack-win": "electron-builder --win --x64",
    "dev-mock-update": "cross-env MOCK_AUTO_UPDATER=true npm run ele-dev",
    "dev-mock-failed-update": "cross-env MOCK_FAILED_UPDATE=true npm run dev-mock-update",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "better-scroll": "^0.1.8",
    "bluebird": "^3.5.1",
    "electron-settings": "^3.1.4",
    "electron-updater": "^2.17.6",
    "lodash": "^4.17.4",
    "request-promise": "^4.2.2",
    "stylus": "^0.54.5",
    "vue": "^2.1.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chalk": "^1.1.3",
    "concurrently": "^3.5.1",
    "connect-history-api-fallback": "^1.1.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.25.0",
    "electron": "^1.7.9",
    "electron-builder": "^19.49.2",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "function-bind": "^1.0.2",
    "html-webpack-plugin": "^2.8.1",
    "http-proxy-middleware": "^0.17.2",
    "json-loader": "^0.5.4",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "semver": "^5.4.1",
    "shelljs": "^0.7.4",
    "stylus-loader": "^2.4.0",
    "url-loader": "^0.5.7",
    "uuid": "^3.1.0",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "build": {
    "productName": "eleme",
    "artifactName": "${productName}_setup.${ext}",
    "appId": "com.eleme.app",
    "files": [
      "public/",
      "main.js",
      "package.json"
    ]
  }
}
